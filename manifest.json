{
  "manifest_version": 2,

  "name": "",
  "description": "See what's happening around you and add events to your calendar",
  "version": "0.01",

  "browser_action": {
    "default_icon": "/src/img/icon.png",
    "default_title": "Click here!"
  },
  "chrome_url_overrides" : {
    "newtab": "/src/html/main.html"
  },
  "permissions": [
    "activeTab",
    "geolocation",
    "cookies",
    "https://maps.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://ipinfo.io/json; object-src 'self'"
}